
@model IEnumerable<MyApp.Models.University>
<H1>Details Of University</H1>
<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(m=>m.Id)</th>
        <th>@Html.DisplayNameFor(m=>m.RegNo)</th>
        <th>@Html.DisplayNameFor(m=>m.Name)</th>
        <th>@Html.DisplayNameFor(m=>m.City)</th>
    </tr>
    <tbody>
        @foreach(var item in Model){
            <tr>
                <td>@item.Id</td>
                <td>@item.RegNo</td>
                <td>@item.Name</td>
                <td>@item.City</td>
            </tr>
        }
    </tbody>
</table>