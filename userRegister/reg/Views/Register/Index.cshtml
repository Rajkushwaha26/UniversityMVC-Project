@model IEnumerable<reg.Models.Register>
<H1>Details of User</H1>
<table class="table">

    <tr>
        <th>@Html.DisplayNameFor(m=>m.RegisterId)</th>
        <th>@Html.DisplayNameFor(m=>m.Name)</th>
        <th>@Html.DisplayNameFor(m=>m.Email)</th>
        <th>@Html.DisplayNameFor(m=>m.Age)</th>
        <th>@Html.DisplayNameFor(m=>m.Password)</th>
        
    </tr>
    <tbody>
        @foreach (var item in Model){
            <tr>
            <td>@item.RegisterId</td>
            <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>